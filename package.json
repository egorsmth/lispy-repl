{
  "name": "lispy-repl",
  "version": "1.0.0",
  "description": "homemade lispy repl",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "babel": "babel",
    "webpack": "webpack",
    "build": "webpack -d && cp src/app/index.html dist/index.html && webpack-dev-server --hot --inline --colors --progress --content-base src/ --host 0.0.0.0 ",
    "build-prod": "webpack -p && cp src/app/index.html dist/index.html",
    "lint": "esw webpack.config.* src tools --color",
    "lint:watch": "npm run lint -- --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/egorsmth/lispy-repl.git"
  },
  "keywords": [
    "repl",
    "lisp",
    "python",
    "enterpreter"
  ],
  "author": "Egor Lebedev <mattspring0@gmail.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/egorsmth/lispy-repl/issues"
  },
  "homepage": "https://github.com/egorsmth/lispy-repl#readme",
  "dependencies": {
    "react": "^16.0.0",
    "react-dom": "^16.0.0"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "eslint": "^4.10.0",
    "eslint-plugin-react": "^7.4.0",
    "eslint-watch": "^3.1.3",
    "webpack": "^3.8.1",
    "webpack-dev-server": "^2.9.4"
  }
}
